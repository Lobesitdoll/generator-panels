<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest ExtensionBundleId="com.photoshop.renamelayers" ExtensionBundleName="Rename Layers" ExtensionBundleVersion="1.2" Version="4.0">
  <Author>
    <![CDATA[J. Peterson]]>
  </Author>
  <ExtensionList>
    <Extension Id="com.photoshop.renamelayers.extension" Version="1.1"/>
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="PHXS" Version="[15.0,15.9]"/>
      <Host Name="PHSP" Version="[15.0,15.9]"/>
    </HostList>
    <LocaleList>
      <Locale Code="All"/>
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="5.0"/>
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.photoshop.renamelayers.extension">
      <DispatchInfo>
        <Resources>
          <MainPath>./renamelayers.html</MainPath>
          <ScriptPath>./host/RenameLayers.jsx</ScriptPath>
        </Resources>
        <UI>
          <Type>Panel</Type>
          <Menu>%rename-panel-menu-name</Menu>
          <Geometry>
            <MinSize>
              <Height>210</Height>
              <Width>216</Width>
            </MinSize>
            <Size>
              <Height>210</Height>
              <Width>216</Width>
            </Size>
          </Geometry>
          <Icons>
            <Icon Type="Normal">./icons/RenamePanelIcon_N.png</Icon>
            <Icon Type="RollOver">./icons/RenamePanelIcon_N.png</Icon>
            <Icon Type="DarkNormal">./icons/RenamePanelIcon_D.png</Icon>
            <Icon Type="DarkRollOver">./icons/RenamePanelIcon_D.png</Icon>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
